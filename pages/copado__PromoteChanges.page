                function initCbonClick() {
                    $copado('input[type="checkbox"]').on('click', function () {
                        var count = $copado('td>input[type="checkbox"]:checked').length;
                        if (count == 0) {
                            enableDisable(true);

                        } else {
                            enableDisable(false);
                        }
                    });
