                    var returnObj = {
                        currentPage: 1,
                        globalNextPrev: undefined,
                        npButtons: $copado('[id$=mButtonsPanel]').html(),
                        totalCm: '{!totalCount}'
                    }
                    $copado('[id$=mButtonsPanel]').detach();
                    return returnObj;
                })();
