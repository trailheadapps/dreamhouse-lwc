                        <script>
                            $copado('.slds-tabs--scoped__item').on('click', function () {
                                $copado(this).addClass('slds-active');
                                $copado(this).addClass('slds-has-focus');
                                $copado(this).find('a').attr('aria-selected', true);
                                var $copadocontentToShow = $copado('#' + $copado(this).find('a').attr('aria-controls'));
                                $copadocontentToShow.removeClass('slds-hide');
                                $copadocontentToShow.addClass('slds-show');

                                $copado(this).siblings().removeClass('slds-active');
                                $copado(this).siblings().removeClass('slds-has-focus');
                                $copado(this).siblings().find('a').attr('aria-selected', false);
                                $copadocontentToShow.siblings('.slds-tabs--scoped__content').removeClass('slds-show');
                                $copadocontentToShow.siblings('.slds-tabs--scoped__content').addClass('slds-hide');
                            });

                            function navigateTo(redirectUrl) {
                                redirectUrl = encodeURI(redirectUrl);
                                if ((typeof sforce != 'undefined') && sforce && (!!sforce.one)) {
                                    sforce.one.navigateToURL(redirectUrl);
                                } else {
                                    window.location.href = redirectUrl;
